{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-multiple-empty-lines": ["error", {"max": 2} ],
        "no-unused-vars": ["warn"],
        "indent": ["error", 4]
    }
}
